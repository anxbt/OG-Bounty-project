#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/0G/frontend/node_modules/.pnpm/@0glabs+0g-serving-broker@0_4730a1695a641d450e9cf9f08db72783/node_modules/@0glabs/0g-serving-broker/cli.commonjs/cli/node_modules:/mnt/d/0G/frontend/node_modules/.pnpm/@0glabs+0g-serving-broker@0_4730a1695a641d450e9cf9f08db72783/node_modules/@0glabs/0g-serving-broker/cli.commonjs/node_modules:/mnt/d/0G/frontend/node_modules/.pnpm/@0glabs+0g-serving-broker@0_4730a1695a641d450e9cf9f08db72783/node_modules/@0glabs/0g-serving-broker/node_modules:/mnt/d/0G/frontend/node_modules/.pnpm/@0glabs+0g-serving-broker@0_4730a1695a641d450e9cf9f08db72783/node_modules/@0glabs/node_modules:/mnt/d/0G/frontend/node_modules/.pnpm/@0glabs+0g-serving-broker@0_4730a1695a641d450e9cf9f08db72783/node_modules:/mnt/d/0G/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/0G/frontend/node_modules/.pnpm/@0glabs+0g-serving-broker@0_4730a1695a641d450e9cf9f08db72783/node_modules/@0glabs/0g-serving-broker/cli.commonjs/cli/node_modules:/mnt/d/0G/frontend/node_modules/.pnpm/@0glabs+0g-serving-broker@0_4730a1695a641d450e9cf9f08db72783/node_modules/@0glabs/0g-serving-broker/cli.commonjs/node_modules:/mnt/d/0G/frontend/node_modules/.pnpm/@0glabs+0g-serving-broker@0_4730a1695a641d450e9cf9f08db72783/node_modules/@0glabs/0g-serving-broker/node_modules:/mnt/d/0G/frontend/node_modules/.pnpm/@0glabs+0g-serving-broker@0_4730a1695a641d450e9cf9f08db72783/node_modules/@0glabs/node_modules:/mnt/d/0G/frontend/node_modules/.pnpm/@0glabs+0g-serving-broker@0_4730a1695a641d450e9cf9f08db72783/node_modules:/mnt/d/0G/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.commonjs/cli/index.js" "$@"
else
  exec node  "$basedir/../../cli.commonjs/cli/index.js" "$@"
fi
